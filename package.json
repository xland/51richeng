{
  "name": "redredstar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm:build-entry\" \"npm:dev\"",
    "build-entry": "rollup --config ./scripts/buildMain.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
